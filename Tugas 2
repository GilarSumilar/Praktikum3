a=input("Masukan nilai a : ")
b=input("Masukan nilai b : ")

a=int(a)
b=int(b)
print("Variable a = ",a)
print("Variable b = ",b)
print("Hasil penggabungan {1}&{0}=%d".format(b,a) %(b&a))

# Konversi Nilai Variable
a=int(a)
b=int(b)
print("Hasil Penjumlahan {1}+{0}=%d".format(b,a) %(b+a))
print("Pembagian {1}/{0}=%d".format(b,a) %(a/b))
